@{
    ViewBag.Title = "Vocabulary";
}

@section header
{
    @{
        if (User.IsInRole("Admin"))
        {
            Html.RenderPartial("PartialAdminHeader");
        }
        else
        {
            Html.RenderPartial("PartialUserHeader");
        }
    }
}

@Html.Action("VocabularyData")

@section scripts
{
    <script src="/Scripts/Users/vocabularyScript.js"></script>
}
