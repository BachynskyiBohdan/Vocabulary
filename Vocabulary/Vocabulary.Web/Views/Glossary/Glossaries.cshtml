@model decimal
@{
    ViewBag.Title = "Glossaries";
}

@section header
{
    @{
        if (User.IsInRole("Admin"))
        {
            Html.RenderPartial("PartialAdminHeader");
        }
        else
        {
            Html.RenderPartial("PartialUserHeader");
        }
    }
}

@Html.Action("GlossariesData", new {langId = @Model})

@section scripts
{
    <script src="/Scripts/Users/glossariesScript.js"></script>
}